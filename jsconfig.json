{
  "compilerOptions": {
    "noLib": false,
    "target": "es6",
    "jsx": "react",
    "checkJs": false,
    "experimentalDecorators": true,
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "tmp",
    "temp",
    "jspm_packages"
  ]
}
